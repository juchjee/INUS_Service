			});
			var ww = $(window).width();
			if( ww <= 1024 ){
				$('.ostop_list > li:nth-child(odd) > dl dd').each(function(){
					var d1 = $(this),
						 d2 = $(this).parents('li').next().find('dd');
					if(d1.height() >= d2.height()){
						d2.css('height', d1.height());
					}else{
						d1.css('height', d2.height());
					}
				});
			}
			$(window).resize(function(){
				var ww = $(window).width();
				if( ww <= 1024 ){
					$('.ostop_list > li:nth-child(odd) > dl dd').each(function(){
						var d1 = $(this),
