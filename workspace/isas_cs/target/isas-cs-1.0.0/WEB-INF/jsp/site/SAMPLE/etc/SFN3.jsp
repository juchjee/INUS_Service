			<script type="text/javascript">
			$('.world_area div.nation p').each(function(){
				var pinW = $(this).outerWidth();
				$(this).css('margin-left', -pinW/2);
			});
			$('.world_area div.nation a.pin').on('mouseenter', function(){
				$(this).next().stop().fadeIn('fast');
			});
			$('.world_area div.nation').on('mouseleave', function(){
				$(this).find('p').hide();
			});

			$('.nation_tab a').on('click', function(){
				$('.nation_tab li').removeClass('on');
				$('.nation_panel > li').hide();
				$(this).parent().addClass('on');
				$($(this).attr('href')).show();
				return false;
			});
			</script>
